#!name=YouTube Ads
#!desc=Use this module to remove the first 5 second ads of YouTube videos.
//GitHub:https://github.com/chiupam/Proxy/blob/individual/Surge/YouTubeAds.sgmodule
//raw:https://raw.githubusercontent.com/chiupam/Proxy/individual/Surge/YouTubeAds.sgmodule
[Rule]
URL-REGEX,^https:\/\/.+\.googlevideo\.com\/.+&oad,REJECT

[URL Rewrite]
ctier=[A-Z] citer=A 302

[MITM]
hostname = %APPEND% *.googlevideo.com
